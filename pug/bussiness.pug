                        p.b-text-norm Hỗ trợ việc kinh doanh của bạn 24/7
                    .b-watch
                        .b-item.wow.fadeInRight(data-wow-delay="1s",data-wow-duration="1s")
                            a(href="#").b-link Xem video
                        .b-item.wow.fadeInLeft(data-wow-delay="1s",data-wow-duration="1s")
                            a(href="#").b-link Đăng ký miễn phí
                    .b-images
                        img(src="./images/business/banner-top.png", alt="Bussiness",style="width:100%")
                    .b-scroll-bottom#top-menu
                        a(href="#function").b-link CUỘN XUỐNG
