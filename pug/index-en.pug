doctype html
html(lang='en')
    include head/_site-head
    body
        include loader/_site-loader
        .b-index
            .site-mobile-menu
                .site-mobile-menu-header
                    .site-mobile-menu-close
                        span.icon-close2.js-menu-toggle
                            i.fas.fa-window-close
                .site-mobile-menu-body
                .site-mobile-menu-lang
            header.b-page-header#b-page-header
                .container
                    .row
                        .col-lg-1
                            .b-logo
                                a(href="index-en.html").b-link
                                    img(src="./images/partial/logo.png", alt="Logo")
                        .col-lg-6
                            nav.b-page-nav
                                .b-open-menu.d-lg-none
                                    a(href="#").site-menu-toggle.js-menu-toggle #[i.fas.fa-bars]
                                ul.b-list-item.js-clone-nav.d-lg-block
                                                -var listItem = [{item:'User',link:'user-en.html'},{item:'BUSINESS',link:'bussiness-en.html'},{item:'FAQ',link:'bussiness.html'},{item:'ABOUT US',link:'about-en.html'},{item:'BLOCK',link:'bussiness.html'}] 
                                                each itemList in listItem
                                                    li.b-item
                                                        -var get = itemList.link
                                                        a(href=get).b-link #{itemList.item}
                        .col-lg-5
                            .b-login.js-clone-lang.d-lg-block.d-none
                                .b-item-lang
                                    a(href="index.html").b-link VIETNAMESE
                                .b-item
                                    a(href="signin.html").b-link SIGN IN
                                .b-item.is-active
                                    a(href="#").b-link SIGN UP
            section.b-page-intro
                .container-fluid
                    .row
                        .col-lg-6
                            .b-overate.wow.fadeInLeft(data-wow-delay="1s",data-wow-duration="1s")
                                .b-content.b-en
                                    h3.b-text-title
                                        :verbatim
                                            Revolutionary Artificial Intelligent Assitant
                                    p.b-text-norm 
                                        :verbatim
                                            Your friendly virtual dining assitant. Find right dining place in the right time. Inbox "Gonjoy" on Facebook Messenger
                                    a(href="#").b-link.b-ripple FIND OUT
                        .col-lg-6
                            .b-enterprise.wow.fadeInRight(data-wow-delay="1s",data-wow-duration="1s")
                                .b-content.b-en
                                    h3.b-text-title Joy and Business
                                    p.b-text-norm 
                                        :verbatim
                                            Connect your customer 24/7.
                                            Support your business 24/7.
                                    a(href="#").b-link.b-ripple FIND OUT
            footer.b-page-footer
                .container
                    .row
                        .col-lg-1
                            .b-logo
                                img(src="./images/partial/logo.png", alt="Logo")
                        .col-lg-3
                            .b-content-footer
                                p.b-text-norm
                                    :verbatim
                                        &copy; 2017 GOnJoy. All Rights Reserved
                        .col-lg-6
                            nav.b-page-nav
                                ul.b-list-item
                                    -var listItem = ['USER','BUSINESS','FAQ','ABOUT US','BLOCK']
                                    each item in listItem
                                        li.b-item
                                            a(href="#").b-link #{item}
                        .col-lg-2
                            nav.b-page-nav
                                ul.b-list-social
                                    li.b-item
                                        a(href="#").b-link #[i.fab.fa-facebook-f]
                                    li.b-item
                                        a(href="#").b-link #[i.fab.fa-youtube]
                                    li.b-item
                                        a(href="#").b-link #[i.fab.fa-google-plus-g]
        script(src="https://code.jquery.com/jquery-2.2.0.min.js")
        script(src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js")
        script(src="./js/wow.js")
        script(src="./js/main.js")