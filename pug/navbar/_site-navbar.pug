.site-mobile-menu
    .site-mobile-menu-header
        .site-mobile-menu-close
            span.icon-close2.js-menu-toggle
                i.fas.fa-window-close
    .site-mobile-menu-body
    .site-mobile-menu-lang
header.b-page-header#b-page-header
    .container
        .row
            .col-lg-1
                .b-logo
                    a(href="index.html").b-link
                        img(src="./images/partial/logo.png", alt="Logo")
            .col-lg-6
                nav.b-page-nav
                    .b-open-menu.d-lg-none
                        a(href="#").site-menu-toggle.js-menu-toggle #[i.fas.fa-bars]
                    ul.b-list-item.js-clone-nav.d-lg-block
                                    -var listItem = [{item:'NGƯỜI DÙNG',link:'user.html'},{item:'DOANH NGHIỆP',link:'bussiness.html'},{item:'FAQ',link:'bussiness.html'},{item:'VỀ CHÚNG TÔI',link:'about.html'},{item:'BLOCK',link:'bussiness.html'}] 
                                    each itemList in listItem
                                        li.b-item
                                            -var get = itemList.link
                                            a(href=get).b-link #{itemList.item}
            .col-lg-5
                .b-login.js-clone-lang.d-lg-block.d-none
                    .b-item-lang
                        a(href="#").b-link ENGLISH
                    .b-item
                        a(href="#").b-link ĐĂNG NHẬP
                    .b-item.is-active
                        a(href="#").b-link Đăng ký miễn phí
